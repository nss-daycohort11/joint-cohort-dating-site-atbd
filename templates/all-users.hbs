{{#each users}}
  <div class="col-xs-6 col-md-3">
  	<div class="thumbnail" id="{{@key}}">

	      <img src="{{facebook.profileImageURL}}" alt="{{facebook.displayName}}">
		    <div class="caption">
		      <h3 class="other-user-name">{{facebook.displayName}}</h3>
		      <p>Just a little about me: {{bio}}</p>
		      <p>Birthday: {{bday}}</p>
		      <p>Look into my eyes: {{eye}}</p>
		      <p>Please don't touch my face: {{face}}</p>
		      <p>Please don't touch my arms or legs: {{limbs}}</p>
		      <p>Please don't touch any of my lower body: {{lowerBody}}</p>
		      <p>Please don't touch any of my upper body: {{upperBody}}</p>
		      <p>Do I have germophobia? {{germophobia}}</p>
		      <p>Do I have agoraphobia? {{agoraphobia}}</p>
		 	  <p>Preferred proximity: {{proximity}} feet</p>
		      <select name="">
		      	<option value="">Air High Five</option>
		      	<option value="">None</option>   	
		      </select>

		    </div>

	  </div>
  </div>
{{/each}}